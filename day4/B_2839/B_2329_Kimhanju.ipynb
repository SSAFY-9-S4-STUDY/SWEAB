   ],
   "source": [
    "N = int(input())\n",
    "\n",
    "div_5 = N // 5\n",
    "N = N % 5 \n",
    "\n",
    "while N % 3 != 0:\n",
    "    N += 5\n",
    "    div_5 -= 1\n",
    "\n",
    "print(div_5 + N//3 if div_5 >= 0 else '-1')"
   ]
  },
